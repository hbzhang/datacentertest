<p id="notice"><%= notice %></p>

<h1>Listing T Facility Access Event Histories</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
    <%=  @t_facility_access_event_histories.count %>
   </tr>
    <% @t_facility_access_event_histories_first.each do |t_facility_access_event_history| %>
      <tr>
        <td><%= t_facility_access_event_history.FIRST_NAME %></td>
        <td><%= t_facility_access_event_history.LAST_NAME %></td>
        <td><%= t_facility_access_event_history.PERSON_ID %></td>
        <td><%= link_to 'Show', t_facility_access_event_history %></td>
        <td><%= link_to 'Edit', edit_t_facility_access_event_history_path(t_facility_access_event_history) %></td>
        <td><%= link_to 'Destroy', t_facility_access_event_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New T facility access event history', new_t_facility_access_event_history_path %>
