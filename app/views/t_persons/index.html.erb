<p id="notice"><%= notice %></p>

<h1>Listing T People</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @t_people.each do |t_person| %>
      <tr>
        <td><%= t_person.FIRST_NAME %></td>
        <td><%= t_person.LAST_NAME %></td>
        <td><%= link_to 'Show', t_person %></td>
        <td><%= link_to 'Edit', edit_t_person_path(t_person) %></td>
        <td><%= link_to 'Destroy', t_person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New T person', new_t_person_path %>
