<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.ORDER_ID %></td>
        <td><%= event.PRODUCT_NAME %></td>
        <td><%= event.PRODUCT_ID %></td>
        </tr>
    <% end %>

    <% @posts.each do |p| %>
   <%= p.comments %>
   <%= p.created_at %>
   <% end %>

  </tbody>
</table>

<br>
